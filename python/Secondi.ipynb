   "source": [
    "def secondbig(A):\n",
    "    big = -float('inf')\n",
    "    sec = -float('inf')\n",
    "    for x in A:\n",
    "        if x>big:\n",
    "            big=x\n",
    "        if x>sec and x<big:\n",
    "            sec = x\n",
    "    return ((big,sec))"
   ]
